
@{
    ViewData["Title"] = "PsychologistUpdate";
    Layout = "_Layout";
}

<h1>psikolog bilgilerini güncelle</h1>



@model PskUpdateViewModel
<h1 class="text-center">Psikolog Ekle</h1>


<div class="row">
    <div class="col-md-10 mx-auto">
   <form  asp-action="PsychologistUpdate" asp-area="Admin" asp-controller="Home" enctype="multipart/form-data" >

    <input asp-for="Psychologist_ID"  type="hidden" value="@ViewBag.id"  />

    <div class="mb-3">

        <label  class="form-label" asp-for="ConsulationPrice">Görüşme Ücret</label>
        <input class="form-control"  asp-for="ConsulationPrice" />
        <span asp-validation-for="ConsulationPrice"></span>

    </div>

     <div>
         <label  class="form-label" asp-for="Title">Ünvan</label>
        <input class="form-control"  asp-for="Title" />
        <span asp-validation-for="Title"></span>
    </div>
    <div class="mb-3">

        <label class="form-label" asp-for="Rank">Site Liste Sıralaması</label>
        <input class="form-control" asp-for="Rank" />
         <span asp-validation-for="Rank"></span>
    </div class="mb-3">

    <div class="mb-3">
        <label class="form-label" asp-for="IsWorking">Aktif mi (bu alan boş bırakılırsa psikolgo sitede görünmez)</label>
        <input  asp-for="IsWorking" />

    </div>

    <div class="mb-3">
         <label class="form-label" >Alan Ekleme(en az bir alana  sahip olmak zorunda )</label>
        <select asp-items="@ViewBag.liste" class="form-select js-example-basic-multiple" name="cateValues" multiple="multiple">
        </select>
    </div>

    <div class="mb-3">

        
        <label class="form-label" asp-for="ShortDescription">Kısa Açıklama Anasayfa için</label>
      
        <textarea class="form-control" asp-for="ShortDescription" rows="6" cols="70">

        </textarea>   
         <span asp-validation-for="ShortDescription"></span>

    </div>

    <div class="mb-3">
        <label asp-for="Photo" class="form-label">Psikolog Resim (değiştirmek istemiyorsanız yeni resim yüklemeyin)</label>
           <span asp-validation-for="Photo"></span>
        <input class="form-control form-control-lg" asp-for="Photo" type="file">
    </div>

    <div class="mb-3">

          <span asp-validation-for="Cv"></span>
        <label asp-for="Cv" class="form-label"> CV </label>
        <textarea asp-for="Cv" class="form-control">


        </textarea>

    </div>

    <div class="mb-3">

        <button type="submit" class="btn btn-primary"> güncelle</button>

    </div>

    </form>


    </div>
</div>


 

